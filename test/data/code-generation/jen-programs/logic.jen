justOnePerson: string, string -> boolean
justOnePerson(x, y):
  return x == 'jen' &!& y == 'justin'

eitherIsTrue: boolean, boolean -> boolean
eitherIsTrue(x, y):
  return x || y

evenNumber: number -> boolean
evenNumber(x):
    return x % 2 == 0

isOdd: number -> string
isOdd(x):
  if !evenNumber(x):
    return 'The number is odd'

print(justOnePerson('justin', 'jen'))
